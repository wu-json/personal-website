{
  "name": "@wu-json/personal-website",
  "version": "1.0.1",
  "private": false,
  "license": "MIT",
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": "20.x",
    "pnpm": "9.x"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.0",
    "@react-three/drei": "^9.89.3",
    "@react-three/fiber": "^8.15.12",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.32.10",
    "three": "^0.159.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "postinstall": "husky install",
    "format": "prettier './**/*.{js,jsx,ts,tsx,json}' --loglevel silent --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.159.0",
    "@vitejs/plugin-react": "^4.3.2",
    "husky": "^6.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "pretty-quick --staged"
    ]
  },
